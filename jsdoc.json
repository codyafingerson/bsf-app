{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
      "include": ["server"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "README.md"
    }
  }